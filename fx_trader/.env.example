# FX Trading System Environment Variables
# Copy this file to .env and fill in your credentials

# GMO Coin FX API Credentials
# Required for LIVE mode, optional for PAPER mode
GMO_API_KEY=your_api_key_here
GMO_API_SECRET=your_api_secret_here

# Telegram Notifications (optional)
TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_CHAT_ID=your_chat_id_here

# System Settings (optional, can be set in config/settings.yaml)
# FX_TRADING_MODE=paper  # paper / live
# LOG_LEVEL=INFO
